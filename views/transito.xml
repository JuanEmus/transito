    <record id="transito_action" model="ir.actions.act_window">
        <field name="name">Productos en tránsito</field>
        <field name="res_model">stock.move</field>
        <field name="view_mode">tree,form</field>
        <!-- <field name="context" eval="{'search_default_productos_transito': 1}"/> -->
        <field name="help" type="html">
            <p class="o_view_nocontent_smiling_face">
                No hay productos que mostrar
            </p>
        </field>
    </record>

    <record id="transito_view_list" model="ir.ui.view">
        <field name="name">transito.view.list.inherited</field>
        <field name="model">stock.move</field>
        <field name="inherit_id" ref="stock.view_move_tree_receipt_picking"/>
        <xpath expr="//field[@name='product_id']" position="after">
            <field name="preciounitario_oc"/>
        </xpath>
    </record>

    <menuitem id="transito" name="Productos en tránsito" parent="purchase.menu_purchase_root" action="transito_action"/>
